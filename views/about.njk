{% extends "layout.njk" %}

{% block head %}

     <title>Sobre -
          {{about.name}}</title>

{% endblock head %}

{% block content %}

     <div id="wrapper">
          <img src="{{about.avatar_url}}" alt="imagem de {{about.name}}">
          <!-- <img src="mauro.jpg" alt="Mauro">-->
          <h1>{{about.name}}</h1>
          <h2>{{about.role}}</h2>
          <p>{{about.description}}</p>
     </div>

     <div id="links-footer" class="links">
          {% for link in about.links %}
               
               <a href="{{link.url}}" target="__blank">{{link.name}}</a>

          {% endfor %}

     </div>

{% endblock content %}